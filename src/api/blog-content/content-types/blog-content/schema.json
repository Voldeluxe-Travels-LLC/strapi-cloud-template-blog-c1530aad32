{
  "kind": "collectionType",
  "collectionName": "blog_contents",
  "info": {
    "singularName": "blog-content",
    "pluralName": "blog-contents",
    "displayName": "BlogContent"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Editor": {
      "type": "component",
      "component": "shared.rich-text",
      "repeatable": false,
      "required": true
    },
    "Slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "SEO": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false,
      "required": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "ReadTime": {
      "type": "integer",
      "required": true,
      "unique": false,
      "min": 1
    },
    "Tag": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "General",
        "Latest Travel News",
        "Flight Routes",
        "One Way",
        "Round Trip",
        "Premium Economy",
        "Business Class",
        "Last Minute Experts",
        "Travel Insurance",
        "Flight Delay & Cancellation"
      ]
    }
  }
}
