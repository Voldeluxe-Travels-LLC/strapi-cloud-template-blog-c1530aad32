{
  "kind": "collectionType",
  "collectionName": "flights",
  "info": {
    "singularName": "flight",
    "pluralName": "flights",
    "displayName": "Flight"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "TripType": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "One-way",
        "Round-trip"
      ]
    },
    "Price": {
      "type": "text",
      "required": true
    },
    "Airline": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Air India ",
        "Emirates ",
        "Qatar Airlines",
        "Turkish Airlines ",
        "Cathay Pacific ",
        "Ethiad ",
        "American Airlines",
        "Lufthansa  ",
        "Air Canada ",
        "United Airlines",
        "Swiss Delta ",
        "KLM ",
        "Airfrance ",
        "Virgin Atlantic ",
        "Kuwait Airways ",
        "Gulf Airlines ",
        "British Airways"
      ]
    },
    "AirlinePrice": {
      "type": "decimal",
      "required": true,
      "min": 1
    },
    "LimitedAvailability": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "Offers": {
      "type": "component",
      "component": "shared.points",
      "repeatable": true,
      "required": true,
      "min": 1
    },
    "Baggage": {
      "type": "string",
      "required": true
    },
    "Cabin": {
      "type": "enumeration",
      "enum": [
        "Economy",
        "Premium Economy",
        "Business",
        "First"
      ]
    }
  }
}
